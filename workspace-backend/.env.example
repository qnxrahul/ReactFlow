# Copy this file to `.env` and fill in the values before running in production.

ENVIRONMENT=development

# Comma-separated list or repeated entries:
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# Azure Blob Storage configuration. Either provide AZURE_STORAGE_CONNECTION_STRING
# or the account URL and key / SAS token.
AZURE_STORAGE_CONNECTION_STRING=
AZURE_STORAGE_ACCOUNT_URL=
AZURE_STORAGE_ACCOUNT_KEY=
AZURE_STORAGE_SAS_TOKEN=
AZURE_STORAGE_CONTAINER=workspace-files

# Cosmos DB configuration.
AZURE_COSMOS_ENDPOINT=
AZURE_COSMOS_KEY=
AZURE_COSMOS_DATABASE=workspace
AZURE_COSMOS_CONTAINER=workspaces
AZURE_COSMOS_THROUGHPUT=400

# Local fallbacks (used when credentials are missing)
ENABLE_LOCAL_FALLBACKS=true
LOCAL_BLOB_ROOT=./.blob-data
